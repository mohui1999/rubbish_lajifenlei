/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  z-index: 999;
  text-align: center;
}
.custom-text-con{
  padding-top: 25rpx; 
}
.custom-text{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
  max-width: 280rpx;
}
.empty_custom{
  height: 45px;
  width: 100%;
}

.custom-back{
  background-image: url(http://xiemenglei.cn/wp-content/uploads/2019/08/back-head.jpg);
  background-size: cover;
}
